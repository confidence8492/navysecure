| NULL        | <p style="text-align: center;">\r\n\t<span style="font-size:18px;"><img alt="海軍大氣海洋局95週年慶祝大會海報" src="/mno/upload/images/1488956040952.JPEG" style="width: 600px; height: 849px;" /><br />\r\n\t海軍大氣海洋局95週年慶祝大會海報</span></p>\r\n<p style="text-align: center;">\r\n\t\xa0</p>\r\n<div>\r\n\t<span style="font-size:18px;">106年4月21日(星期五)海軍大氣海洋局95週年慶祝大會行程：</span></div>\r\n<div>\r\n\t<span style="font-size:18px;">0900-0930 \xa0恭迎，報到</span></div>\r\n<div>\r\n\t<span style="font-size:18px;">0930-1030 \xa095週年慶祝大會</span></div>\r\n<div>\r\n\t<span style="font-size:18px;">1030-1130 \xa0歷史回顧，靜態陳展</span></div>\r\n<div>\r\n\t<span style="font-size:18px;">1130-1250 \xa0慶祝餐會</span></div>\r\n<div>\r\n\t<span style="font-size:18px;">1250-1300 \xa0賦歸</span></div>\r\n<div>\r\n\t<span style="font-size:18px;">誠摯邀請前輩與同袍團圓回娘家，一起對海軍大氣海洋局說：「95歲生日快樂！」</span></div>\r\n<div>\r\n\t<span style="font-size:18px;">聯絡人：羅少校 \xa00921327758 lomifu0723@gmail.com</span></div>\r\n<div>\r\n\t<span style="font-size:18px;">\xa0 \xa0 \xa0 \xa0 \xa0 \xa0 \xa0 \xa0許少校 \xa00982515496 \xa0hisoka1211@gmail.com</span></div>\r\n<div>\r\n\t\xa0</div>\r\n<p>\r\n\t\xa0</p>\r\n                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    